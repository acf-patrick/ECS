Name: main
Entities:
  - TagComponent:
      Tag: main camera
    TransformComponent:
      Position: [0, 0]
      Scale: [1, 1]
      Rotation: 0
    CameraComponent:
      Size: [1, 1]
      Destination: [0, 0]
      BackgroundColor: [120, 20, 100, 255]
      # BackgroundImage: ""
      ClearMode: solid color
      Flip: [false, false]
      Depth: 0
      Layers: [0]
  - TagComponent:
      Tag: sprite
    TransformComponent:
      Position: [0, 0]
      Scale: [0.25, 0.25]
      Rotation: 0
    SpriteComponent:
      Texture: texture.jpg
      Centered: false
      Offset: [0, 0]
      Flip: [false, false]
      FramesNumber: [1, 1]
      Frame: 0
      RegionEnabled: false
      Region: [[0, 0], [0, 0]]
